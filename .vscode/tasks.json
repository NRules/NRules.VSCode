{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"problemMatcher": [
				{
					"base": "$tsc-watch",
					"background": {
						"activeOnStart": true,
						"beginsPattern": "\\[watch\\] build started",
						"endsPattern": "\\[watch\\] build finished"
					}
				}
			],
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
